{% extends 'base.html.twig' %}
{% block body %}
    <div class="container">
        <div class="modal-header mb-3">
            <h3>Tambah Kata</h3>
        </div>
        {{ form_start(form) }}
        <div class="mt-5 border mb-4 pt-4 pb-4 pl-5">
            <div class="form-group row">
                <label for="colFormLabelSm" class="col-sm-2 col-form-label col-form-label-sm"><b>Email</b></label>
                <div class="col-sm-10">
                    {{ form_row(form.kataDasar) }}
                </div>
            </div>
            <div class="form-group row">
                <label for="colFormLabelSm" class="col-sm-2 col-form-label col-form-label-sm"><b>Password</b></label>
                <div class="col-sm-10">
                    {{ form_row(form.keterangan) }}
                </div>
            </div>
            <div class="form-group row">
                <label for="colFormLabelSm" class="col-sm-2 col-form-label col-form-label-sm"><b>Status</b></label>
                <div class="col-sm-10">
                    {{ form_row(form.statusKata) }}
                </div>
            </div>
        </div>
        {{ form_end(form) }}
    </div>
{% endblock %}